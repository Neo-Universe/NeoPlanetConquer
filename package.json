{
  "name": "neo-planet-conquer",
  "displayName": "Neo-Planet-Conquer",
  "description": "Yo, I'm just a resx file editor",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "neo-planet-conquer.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "neo-planet-conquer.openResxAsTable",
        "title": "Open as Table",
        "category": "Neo-Planet-Conquer"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceLangId == xml",
          "command": "neo-planet-conquer.openResxAsTable",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "xml2js": "^0.6.2"
  }
}
